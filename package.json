{
  "name": "@krisdages/d3-hierarchy",
  "version": "3.1.2",
  "description": "Layout algorithms for visualizing hierarchical data.",
  "homepage": "https://d3js.org/d3-hierarchy/",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3/d3-hierarchy.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "layout",
    "tree",
    "treemap",
    "hierarchy",
    "infovis"
  ],
  "license": "ISC",
  "author": {
    "name": "Mike Bostock",
    "url": "http://bost.ocks.org/mike"
  },
  "type": "module",
  "files": [
    "dist",
    "src/**/*.js"
  ],
  "module": "src/index.js",
  "main": "dist/d3-hierarchy.min.cjs",
  "jsdelivr": "dist/d3-hierarchy.min.js",
  "unpkg": "dist/d3-hierarchy.min.js",
  "exports": {
    "umd": "./dist/d3-hierarchy.min.js",
    "require": "./dist/d3-hierarchy.min.cjs",
    "default": "./src/index.js"
  },
  "sideEffects": false,
  "devDependencies": {
    "benchmark": "2",
    "d3-array": "npm:@krisdages/d3-array@<4",
    "d3-dsv": "1 - 3",
    "d3-random": "1.1.0 - 3",
    "eslint": "8",
    "mocha": "9",
    "rollup": "2",
    "rollup-plugin-terser": "7"
  },
  "scripts": {
    "test": "mocha 'test/**/*-test.js' && eslint src test",
    "prepublishOnly": "rm -rf dist && yarn test && rollup -c"
  },
  "engines": {
    "node": ">=12"
  }
}
